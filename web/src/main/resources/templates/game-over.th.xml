<thlogic>

    <attr sel="#headId"
          th:replace="~{fragments/head :: headFragment}"/>

    <attr sel="#head"
          th:text="#{game.title}"/>

    <attr sel="#game-over"
          th:text="#{game.game-over}"/>

    <attr sel="#resultMessage"
          th:text="${__${T(adam.spring.util.AttributeNames).RESULT_MESSAGE}__}"/>

    <attr sel="#play-again"
          th:href="@{__${T(adam.spring.util.GameMappings).RESTART}__}"
          th:text="#{game.play-again}"/>

    <attr sel="#home-link"
          th:href="@{__${T(adam.spring.util.GameMappings).HOME}__}"/>
    <attr sel="#footer-id"
          th:replace="~{fragments/footer :: footerFragment}"/>






</thlogic>